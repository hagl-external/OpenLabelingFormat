{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://github.com/ADAS-API/OpenLabelFormat/blob/master/schema.json",
    "type": "object",
    "title": "OpenLabelFormat",
    "description": "Created with Liquid Studio 2019 (https://www.liquid-technologies.com)",
    "additionalProperties": false,
    "properties": {
        "versionInfo": {
            "type": "object",
            "description": "Versions of schema and label files as string.",
            "additionalProperties": false,
            "properties": {
                "name": {
                    "type": "string",
                    "description": "Constant 'OpenLabelFormat' naming the label file.",
                    "const": "OpenLabelFormat"
                },
                "schema": {
                    "type": "string",
                    "description": "String containing version of schema used for validation."
                },
                "labelFile": {
                    "type": "string",
                    "description": "String containing version of label file."
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "schema",
                "labelFile",
                "name"
            ]
        },
        "projectInfo": {
            "type": "object",
            "description": "Project settings.",
            "additionalProperties": false,
            "properties": {
                "intervals": {
                    "type": "array",
                    "description": "Analyzed frames or time stamps within media file(s). Must be accumulated over consecutive media files, e. g. frame 0 of media file following a 100 frames media file should be frame 101 etc.",
                    "items": {
                        "type": "number",
                        "description": "Frame no. or time stamp.",
                        "minimum": 0
                    }
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of configured start/end/skip or interval settings.",
                    "enum": [
                        "frames",
                        "ms",
                        "s"
                    ]
                },
                "configs": {
                    "type": "array",
                    "description": "List of references to external configuration files.",
                    "items": {
                        "$ref": "#/definitions/fileReference"
                    }
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "unit",
                "intervals"
            ]
        },
        "odometryInfo": {
            "type": "object",
            "description": "Details about corresponding egomotion file(s).",
            "additionalProperties": false,
            "properties": {
                "references": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/fileReferenceWithMediaReference"
                    }
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "references"
            ]
        },
        "calibrationInfo": {
            "type": "object",
            "description": "Details about corresponding calibration file(s).",
            "additionalProperties": false,
            "properties": {
                "references": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/fileReferenceWithMediaReference"
                    }
                },
                "coordinateSystems": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/coordinateSystem"
                    }
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "references"
            ]
        },
        "positionInfo": {
            "type": "object",
            "description": "Details about corresponding position file(s), e. g. GPS.",
            "additionalProperties": false,
            "properties": {
                "references": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/fileReferenceWithMediaReference"
                    }
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "references"
            ]
        },
        "mediaInfo": {
            "type": "object",
            "description": "Details about used media file(s).",
            "additionalProperties": false,
            "properties": {
                "references": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/mediaReference"
                    }
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "references"
            ]
        },
        "metaInfo": {
            "type": "array",
            "description": "Array of attributes valid for parts of or the entire media file(s).",
            "items": {
                "$ref": "#/definitions/metaAttribute"
            }
        },
        "objectInfo": {
            "type": "array",
            "description": "Array of labeled objects.",
            "items": {
                "$ref": "#/definitions/object"
            }
        }
    },
    "required": [
        "versionInfo",
        "projectInfo",
        "mediaInfo",
        "objectInfo",
        "positionInfo",
        "calibrationInfo",
        "odometryInfo",
        "metaInfo"
    ],
    "definitions": {
        "uuid": {
            "type": "string",
            "description": "36-hex-digit uuid string containing dashes, i. e. 8-4-4-4-12.",
            "maxLength": 36,
            "minLength": 36,
            "format": "uuid"
        },
        "hash": {
            "type": "object",
            "description": "Describing hash value and method.",
            "additionalProperties": false,
            "properties": {
                "value": {
                    "type": "string",
                    "description": "Hash value as string."
                },
                "method": {
                    "type": "string",
                    "description": "Used hashing method.",
                    "enum": [
                        "md5",
                        "sha-1",
                        "sha-224",
                        "sha-256",
                        "sha-384",
                        "sha-512",
                        "ripemd-160"
                    ]
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "value",
                "method"
            ]
        },
        "agent": {
            "type": "object",
            "description": "Properties of a label agent.",
            "additionalProperties": false,
            "properties": {
                "uuid": {
                    "$ref": "#/definitions/uuid"
                },
                "dateTime": {
                    "type": "string",
                    "description": "Date-time as defined by RFC3339.",
                    "format": "date-time"
                }
            },
            "required": [
                "dateTime"
            ]
        },
        "history": {
            "type": "object",
            "description": "Tracks the history of a, reference, attribute or object.",
            "additionalProperties": false,
            "properties": {
                "created": {
                    "$ref": "#/definitions/agent"
                },
                "modified": {
                    "$ref": "#/definitions/agent"
                }
            },
            "required": [
                "modified",
                "created"
            ]
        },
        "relation": {
            "type": "object",
            "description": "Relations from an object to other or objects.",
            "properties": {
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "anyOf": [
                {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "uuidOfParent": {
                            "$ref": "#/definitions/uuid"
                        }
                    },
                    "required": [
                        "uuidOfParent"
                    ]
                },
                {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "uuidOfSibling": {
                            "$ref": "#/definitions/uuid"
                        }
                    },
                    "required": [
                        "uuidOfSibling"
                    ]
                },
                {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "uuidOfChild": {
                            "$ref": "#/definitions/uuid"
                        }
                    },
                    "required": [
                        "uuidOfChild"
                    ]
                }
            ]
        },
        "temporalValidity": {
            "type": "object",
            "description": "Temporal validity of an object or attribute.",
            "additionalProperties": false,
            "properties": {
                "start": {
                    "type": "number",
                    "description": "Start of temporal validity. Must be accumulated over concecutive media files, e. g. frame 0 of media file following a 100 frames media file should be frame 101 etc.",
                    "minimum": 0
                },
                "end": {
                    "type": "number",
                    "description": "End of temporal validity. Must be accumulated over concecutive media files, e. g. frame 0 of media file following a 100 frames media file should be frame 101 etc.",
                    "minimum": 0
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of temporal validity."
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "start",
                "end",
                "unit"
            ]
        },
        "temporalValiditySingle": {
            "type": "object",
            "description": "Temporal validity of an object or attribute.",
            "additionalProperties": false,
            "properties": {
                "at": {
                    "type": "number",
                    "description": "Temporal validity. Must be accumulated over concecutive media files, e. g. frame 0 of media file following a 100 frames media file should be frame 101 etc.",
                    "minimum": 0
                },
                "unit": {
                    "type": "string",
                    "description": "Unit of temporal validity."
                },
                "history": {
                    "$ref": "#/definitions/history"
                }
            },
            "required": [
                "unit",
                "at"
            ]
        },
